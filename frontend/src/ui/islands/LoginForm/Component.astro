---
export interface Props extends Astro.HTMLComponentProps<"form"> {
    loginURL: URLString;
    fields: any[];
}

const {
    loginURL,
    fields,
    ...props
} = Astro.props;
---

<form
    @submit.prevent="$actions.loginUser($jsonify($el)).catch(error => console.log(error))"
    class="space-y-6"
    {...props}
>
    <input type="submit" class="btn btn-primary w-full" value="Login" />
</form>
