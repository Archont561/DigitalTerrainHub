{
  "name": "theme",
  "version": "4.0.1",
  "description": "",
  "scripts": {
    "start": "npm run dev",
    "build": "npm run build:clean && npm run build:vite && npm run build:tailwind",
    "build:clean": "rimraf ../static/dist",
    "build:tailwind": "cross-env NODE_ENV=production postcss ./src/css/main.css -o ../static/dist/styles.css --minify",
    "build:vite": "vite build",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:tailwind\"",
    "dev:vite": "vite",
    "dev:tailwind": "cross-env NODE_ENV=development postcss ./src/css/main.css -o ../static/dist/styles.css --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.0",
    "@types/alpinejs": "^3.13.11",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.17",
    "@types/luxon": "^3.6.2",
    "@types/proj4": "^2.5.6",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.5"
  },
  "dependencies": {
    "@alpinejs/anchor": "^3.14.9",
    "@alpinejs/focus": "^3.14.9",
    "@alpinejs/intersect": "^3.14.9",
    "@alpinejs/mask": "^3.14.9",
    "@uppy/core": "^4.4.4",
    "@uppy/dashboard": "^4.3.3",
    "@uppy/tus": "^4.2.2",
    "alpinejs": "^3.14.9",
    "daisyui": "^5.0.35",
    "luxon": "^3.6.1",
    "ol": "^10.5.0",
    "proj4": "^2.17.0",
    "tailwindcss-animated": "^2.0.0"
  }
}
